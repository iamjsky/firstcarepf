<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
  >

    <LinearLayout
        android:id="@+id/layout_item"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">

        <!--    출퇴근 상세 내용-->
        <LinearLayout
            android:gravity="center|bottom"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="0dp">

            <LinearLayout
                android:layout_gravity="right"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <TextView

                    android:gravity="bottom|center|right"
                    android:id="@+id/tv_commuteList"
                    android:layout_marginBottom="@dimen/_1sdp"
                    android:layout_gravity="bottom"
                    android:textSize="@dimen/_9sdp"
                    android:textColor="@color/fc_mainColor"

                    android:text="@string/search_commute_list"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"/>

            </LinearLayout>

                <LinearLayout
                    android:paddingTop="@dimen/_1sdp"
                    android:paddingBottom="@dimen/_1sdp"
                    android:background="@color/fc_middleLightColor2"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <LinearLayout
                        android:background="@color/fc_middleLightColor"
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <TextView

                            android:gravity="center"
                            android:textColor="@color/fc_mainColor"
                            android:text="@string/date_01"
                            android:textSize="@dimen/_10sdp"
                            android:layout_width="@dimen/_46sdp"
                            android:layout_height="@dimen/_30sdp"/>
                        <TextView

                            android:gravity="center"
                            android:textColor="@color/fc_mainColor"
                            android:text="@string/agency"
                            android:textSize="@dimen/_10sdp"
                            android:layout_width="@dimen/_46sdp"
                            android:layout_height="@dimen/_30sdp"/>
                        <TextView

                            android:gravity="center"
                            android:textColor="@color/fc_mainColor"
                            android:text="@string/service"
                            android:textSize="@dimen/_10sdp"
                            android:layout_width="@dimen/_46sdp"
                            android:layout_height="@dimen/_30sdp"/>
                        <TextView

                            android:gravity="center"
                            android:textColor="@color/fc_mainColor"
                            android:text="@string/get_to_work_02"
                            android:textSize="@dimen/_10sdp"
                            android:layout_width="@dimen/_46sdp"
                            android:layout_height="@dimen/_32sdp"/>
                        <TextView

                            android:gravity="center"
                            android:textColor="@color/fc_mainColor"
                            android:text="@string/finish_work_02"
                            android:textSize="@dimen/_10sdp"
                            android:layout_width="@dimen/_46sdp"
                            android:layout_height="@dimen/_32sdp"/>

                    </LinearLayout>



                    <LinearLayout
                        android:paddingLeft="@dimen/_10sdp"
                        android:background="@color/white"
                        android:layout_weight="1"
                        android:orientation="vertical"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content">


                        <LinearLayout
                            android:gravity="center"
                            android:background="@color/white"
                            android:orientation="vertical"

                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp">

                            <TextView
                                android:id="@+id/tv_attDate"
                                android:singleLine="true"
                                android:gravity="center|left"
                                android:text="-"
                                android:textColor="@color/fc_mainColor"
                                android:textSize="@dimen/_10sdp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>

                        </LinearLayout>


                        <LinearLayout
                            android:gravity="center"
                            android:background="@color/white"
                            android:orientation="vertical"

                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp">
                            <TextView
                                android:id="@+id/tv_attAgency"
                                android:singleLine="true"
                                android:gravity="center|left"
                                android:text="-"
                                android:textColor="@color/fc_mainColor"
                                android:textSize="@dimen/_10sdp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>

                        </LinearLayout>

                        <LinearLayout
                            android:gravity="center"
                            android:background="@color/white"
                            android:orientation="vertical"

                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_30sdp">

                            <TextView
                                android:id="@+id/tv_attService"
                                android:singleLine="true"
                                android:gravity="center|left"
                                android:text="-"
                                android:textColor="@color/fc_mainColor"
                                android:textSize="@dimen/_10sdp"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>

                        </LinearLayout>

                        <LinearLayout
                            android:gravity="center"
                            android:background="@color/white"
                            android:orientation="horizontal"

                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_32sdp">

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_weight="1"
                                android:layout_width="0dp"
                                android:layout_height="match_parent">

                                <TextView
                                    android:id="@+id/tv_attTime"
                                    android:singleLine="true"
                                    android:gravity="center|left"
                                    android:text="-"
                                    android:textColor="@color/fc_mainColor"
                                    android:textSize="@dimen/_10sdp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>
                                <TextView

                                    android:id="@+id/tv_attAddr"
                                    android:singleLine="true"
                                    android:gravity="center|left"
                                    android:text="-"
                                    android:textColor="@color/fc_mainColor"
                                    android:textSize="@dimen/_10sdp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>

                            </LinearLayout>

                            <ImageView
                                android:visibility="invisible"
                                android:id="@+id/iv_getToWorkInfo"
                                android:layout_marginLeft="@dimen/_4sdp"
                                android:src="@drawable/icn_marker_01"
                                android:adjustViewBounds="true"
                                android:layout_width="@dimen/_22sdp"
                                android:layout_height="@dimen/_22sdp"/>


                        </LinearLayout>

                        <LinearLayout
                            android:gravity="center"
                            android:background="@color/white"
                            android:orientation="horizontal"

                            android:layout_width="match_parent"
                            android:layout_height="@dimen/_32sdp">

                            <LinearLayout
                                android:orientation="vertical"
                                android:layout_weight="1"
                                android:layout_width="0dp"
                                android:layout_height="match_parent">

                                <TextView
                                    android:id="@+id/tv_leaveTime"
                                    android:singleLine="true"
                                    android:gravity="center|left"
                                    android:text="-"
                                    android:textColor="@color/fc_mainColor"
                                    android:textSize="@dimen/_10sdp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>
                                <TextView
                                    android:id="@+id/tv_leaveAddr"
                                    android:singleLine="true"
                                    android:gravity="center|left"
                                    android:text="-"
                                    android:textColor="@color/fc_mainColor"
                                    android:textSize="@dimen/_10sdp"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>

                            </LinearLayout>

                            <ImageView
                                android:visibility="invisible"
                                android:id="@+id/iv_finishWorkInfo"
                                android:layout_marginLeft="@dimen/_4sdp"
                                android:src="@drawable/icn_marker_01"
                                android:adjustViewBounds="true"
                                android:layout_width="@dimen/_22sdp"
                                android:layout_height="@dimen/_22sdp"/>


                        </LinearLayout>





                    </LinearLayout>











                </LinearLayout>


            </LinearLayout>

            <!--            -->

            <!--            이용자 출석 관리 버튼-->
            <Button
                android:layout_marginRight="@dimen/_30sdp"
                android:layout_marginLeft="@dimen/_30sdp"
                android:layout_marginTop="@dimen/_20sdp"
                android:layout_marginBottom="@dimen/_30sdp"
                android:stateListAnimator="@null"
                android:textColor="@color/fc_boldColor"
                android:enabled="true"
                android:background="@drawable/bg_rounded_01"
                android:textStyle="bold"
                android:textSize="@dimen/_11sdp"
                android:text="@string/user_commute_admin"
                android:id="@+id/btn_userRollList"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_35sdp"/>



    </LinearLayout>



</LinearLayout>