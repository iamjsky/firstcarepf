<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical"
    android:paddingRight="@dimen/_20sdp"
    android:paddingLeft="@dimen/_20sdp"

    >
<!--제공인력 로그인 일때, 네비 메뉴-->
    <LinearLayout
        android:id="@+id/layout_navMenu_typeV"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

    <TextView
        android:id="@+id/tv_notice_typeV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/_6sdp"
        android:paddingBottom="@dimen/_6sdp"
        android:gravity="center|left"
        android:text="@string/notice"
        android:textColor="@color/fc_mainColor"
        android:textSize="@dimen/_12sdp"
        android:textStyle="bold" />
    <TextView
        android:id="@+id/tv_faq_typeV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/_6sdp"
        android:paddingBottom="@dimen/_6sdp"
        android:gravity="center|left"
        android:text="@string/faq"
        android:textColor="@color/fc_mainColor"
        android:textSize="@dimen/_12sdp"
        android:textStyle="bold" />
    <TextView
        android:visibility="gone"
        android:id="@+id/tv_support_typeV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/_6sdp"
        android:paddingBottom="@dimen/_6sdp"
        android:gravity="center|left"
        android:text="@string/support"
        android:textColor="@color/fc_mainColor"
        android:textSize="@dimen/_12sdp"
        android:textStyle="bold" />
    </LinearLayout>
    <!--대상자 보호자 로그인 일때, 네비 메뉴-->
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/layout_navMenu_typeG"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_notice_typeG"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/_6sdp"
            android:paddingBottom="@dimen/_6sdp"
            android:gravity="center|left"
            android:text="대상자 보호자 메뉴1"
            android:textColor="@color/fc_mainColor"
            android:textSize="@dimen/_12sdp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/tv_faq_typeG"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/_6sdp"
            android:paddingBottom="@dimen/_6sdp"
            android:gravity="center|left"
            android:text="대상자 보호자 메뉴2"
            android:textColor="@color/fc_mainColor"
            android:textSize="@dimen/_12sdp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/tv_support_typeG"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/_6sdp"
            android:paddingBottom="@dimen/_6sdp"
            android:gravity="center|left"
            android:text="대상자 보호자 메뉴3"
            android:textColor="@color/fc_mainColor"
            android:textSize="@dimen/_12sdp"
            android:textStyle="bold" />
    </LinearLayout>
    <!--케어선생님 로그인 일때, 네비 메뉴-->
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/layout_navMenu_typeN"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_notice_typeN"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/_6sdp"
            android:paddingBottom="@dimen/_6sdp"
            android:gravity="center|left"
            android:text="케어선생님 메뉴1"
            android:textColor="@color/fc_mainColor"
            android:textSize="@dimen/_12sdp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/tv_faq_typeN"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/_6sdp"
            android:paddingBottom="@dimen/_6sdp"
            android:gravity="center|left"
            android:text="케어선생님 메뉴2"
            android:textColor="@color/fc_mainColor"
            android:textSize="@dimen/_12sdp"
            android:textStyle="bold" />
        <TextView
            android:id="@+id/tv_support_typeN"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/_6sdp"
            android:paddingBottom="@dimen/_6sdp"
            android:gravity="center|left"
            android:text="케어선생님 메뉴3"
            android:textColor="@color/fc_mainColor"
            android:textSize="@dimen/_12sdp"
            android:textStyle="bold" />
    </LinearLayout>
</LinearLayout>